@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="~/Resource/jquery-3.3.1.min.js"></script>
    <script src="~/Resource/jquery.cookie.js"></script>
    <script src="~/Resource/bootstrap.min.js"></script>
    <script src="~/Resource/publicJs.js"></script>
    <script src="../../Resource/picPage/js/picShopPageJs.js"></script>
